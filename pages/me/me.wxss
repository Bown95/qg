.container {
  position: fixed;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.head_image_view {
  position: relative;
  width: 720rpx;
  height: 290rpx;
  border-radius: 20rpx;
  overflow: hidden;
  margin-top: 40rpx;
}

.head_image_view .head_bg {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.head_image_view .head_front {
  position: relative;
  z-index: 1;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.head_front .head {
  width: 148rpx;
  height: 148rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 50%;
  margin-left: 50rpx;
  overflow: hidden;
  border: 4rpx solid white;
}

.head_front .name {
  font-size: 46rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  margin-left: 30rpx;
  color: rgba(255, 255, 255, 1);
  flex-grow: 1;
}

.head_front .nameouter {
  display: flex;
  flex-direction: column;
  justify-content: center;
  flex-grow: 1;
}

.head_front .nameouter .userwx {
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  margin-left: 30rpx;
  color: rgba(255, 255, 255, 1);
}

.head_front .nameouter .userphone {
  font-size: 28rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  margin-left: 30rpx;
  color: rgba(255, 255, 255, 1);
}

.head_front .code {
  width: 80rpx;
  height: 80rpx;
  margin-right: 38rpx;
}

.me_list {
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
}

.me_list .item {
  height: 113rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 26px 0px rgba(24, 24, 24, 0.08);
  border-radius: 15rpx;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}

.me_list .item .left_logo {
  width: 44rpx;
  height: 44rpx;
  margin-left: 20rpx;
}

.me_list .item .name {
  flex-grow: 1;
  margin-left: 16rpx;
  width: 140rpx;
  height: 33rpx;
  line-height: 33rpx;
  font-size: 36rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
}

.me_list .item .goimg {
  width: 25rpx;
  height: 25rpx;
  margin-right: 28rpx;
}
